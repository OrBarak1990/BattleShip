<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="box" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EndGameController">
   <children>
      <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" prefHeight="101.0" prefWidth="601.0" text="Player X Win!" textAlignment="JUSTIFY">
         <font>
            <Font size="64.0" />
         </font>
      </Label>
      <Label fx:id="playerWinScore" prefHeight="110.0" prefWidth="281.0" text="Player win Score ">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="50.0" top="75.0" />
         </padding>
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="playerLooseScore" prefHeight="110.0" prefWidth="278.0" text="Player Loose Score ">
         <padding>
            <Insets left="50.0" />
         </padding>
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="close" mnemonicParsing="false" onAction="#continueAction" text="Continue">
         <VBox.margin>
            <Insets left="450.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
